def sgp(m):
  if not m.text == 'Ù¾Ù†Ù„':
        text = m.text
        bot.reply_to(m, 'Ù¾ÛŒØ§Ù… Ø´Ù…Ø§ Ø§Ø±Ø³Ø§Ù„ Ø´Ø¯\n\n{}'.format(text))
        rd = redis.smembers('groups')
        for id in rd:
            try:
                bot.send_message(id, "{}".format(text), parse_mode="Markdown")
            except:
                redis.srem('groups', id)
                
