def is_sudo(user_id):
    var = False
    if int(user_id) in sudo:
        var = True
    return var

def is_mod(chat_id, user_id):
    var = False
    if bot.get_chat_member(chat_id, user_id).status in ["creator", "administrator"]:
        var = True
    return var

